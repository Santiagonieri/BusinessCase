		formatString: \$\ #,0.###############;-\$\ #,0.###############;\$\ #,0.###############
		displayFolder: Cesta
		lineageTag: 36d92c25-1924-4d38-9e02-a35b1c56cb74

		annotation PBI_FormatHint = {"currencyCulture":"es-AR"}

